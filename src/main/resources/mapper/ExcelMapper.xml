<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.restful.system.mapper.ExcelMapper">

    <select id="selectByDate" resultType="com.restful.poi.model.Excel">
        SELECT * FROM excel WHERE buy_time BETWEEN '2019-09-01 00:00:00' AND '2019-09-03 23:59:59'
    </select>
</mapper>